  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npx dotenv -e .env.test -- jest --runInBand",
    "test:watch": "dotenv -e .env.test -- jest --watch --runInBand --no-cache",
    "test:cov": "dotenv -e .env.test -- jest --coverage --runInBand",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:migrate": "dotenv -e .env -- prisma migrate dev",
    "prisma:migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "prisma:generate": "prisma generate",
    "prisma:generate:test": "dotenv -e .env.test -- prisma generate"
  },
