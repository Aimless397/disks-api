    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^5.0.0",
    "graphql": "^16.3.0",
    "http-errors": "^1.8.0",
    "jsonwebtoken": "^8.5.1",
